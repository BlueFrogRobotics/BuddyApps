<?xml version="1.0" encoding="utf-8"?>
<BehaviourAlgorithmStorage xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <Settings>
    <Category>TrackingEnd</Category>
    <Mood>NEUTRAL</Mood>
    <Commitment>NOT_ENGAGED</Commitment>
    <MovementPattern>COMPLETE_FREEDOM</MovementPattern>
  </Settings>
  <Algorithm>
    <Instructions>
      <!-- START END TRAKING -->
      <ABehaviourInstruction xsi:type="LookAtBehaviourInstruction">
        <Delay>
          <Value>0</Value>
        </Delay>
        <IsBlocking>
          <Value>false</Value>
        </IsBlocking>
        <IsRequired>
          <Value>false</Value>
        </IsRequired>
        <Position>
          <Value>DOWN</Value>
        </Position>
      </ABehaviourInstruction>
      <ABehaviourInstruction xsi:type="PlayFacialEventBehaviourInstruction">
        <Delay>
          <Value>0</Value>
        </Delay>
        <IsBlocking>
          <Value>false</Value>
        </IsBlocking>
        <IsRequired>
          <Value>false</Value>
        </IsRequired>
        <FacialEvent>
          <Value>BLINK_EYES</Value>
        </FacialEvent>
      </ABehaviourInstruction>
      <ABehaviourInstruction xsi:type="MoveHeadBehaviourInstruction">
        <Delay>
          <Value>0.25</Value>
        </Delay>
        <IsBlocking>
          <Value>true</Value>
        </IsBlocking>
        <IsRequired>
          <Value>false</Value>
        </IsRequired>
        <CustomMove>
          <Value>true</Value>
        </CustomMove>
        <YesAngle>
          <Value>-20.0</Value>
        </YesAngle>
        <YesSpeed>
          <Value>50</Value>
        </YesSpeed>
        <NoAngle>
          <Value>-20.0</Value>
          <Name>nopos</Name>
        </NoAngle>
        <NoSpeed>
          <Value>50</Value>
        </NoSpeed>
      </ABehaviourInstruction>
      <!-- RANDOM YAWN -->
      <ABehaviourInstruction xsi:type="RunScriptBehaviourInstruction">
        <IsBlocking>
          <Value>true</Value>
        </IsBlocking>
        <FileName>
          <Value>Yawn01.xml</Value>
          <Name>randomyawn</Name>
        </FileName>
      </ABehaviourInstruction>
      <!-- RESET POSITIONS -->
      <ABehaviourInstruction xsi:type="MoveHeadBehaviourInstruction">
        <Delay>
          <Value>0</Value>
        </Delay>
        <IsBlocking>
          <Value>true</Value>
        </IsBlocking>
        <IsRequired>
          <Value>false</Value>
        </IsRequired>
        <CustomMove>
          <Value>true</Value>
        </CustomMove>
        <Position>
          <Value>CENTER</Value>
        </Position>
        <YesAngle>
          <Value>0</Value>
        </YesAngle>
        <YesSpeed>
          <Value>50</Value>
        </YesSpeed>
        <NoAngle>
          <Value>0</Value>
        </NoAngle>
        <NoSpeed>
          <Value>50</Value>
        </NoSpeed>
      </ABehaviourInstruction>
    </Instructions>
  </BehaviourAlgorithm>
</BehaviourAlgorithmStorage>