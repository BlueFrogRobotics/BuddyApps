<?xml version="1.0" ?>
  <BehaviourAlgorithm xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <Instructions>
      <ABehaviourInstruction xsi:type="SayBehaviourInstruction">
        <IsBlocking>
          <Value>true</Value>
        </IsBlocking>
        <IsRequired>
          <Value>false</Value>
        </IsRequired>
        <Key>
          <Value>welcome</Value>
        </Key>
      </ABehaviourInstruction>
      <ABehaviourInstruction xsi:type="PlaySoundBehaviourInstruction">
        <Delay>
          <Value>0.5</Value>
        </Delay>
        <IsBlocking>
          <Value>false</Value>
        </IsBlocking>
        <IsRequired>
          <Value>false</Value>
        </IsRequired>
        <SoundSample>
          <Value>YAWN</Value>
        </SoundSample>
      </ABehaviourInstruction>
      <ABehaviourInstruction xsi:type="PlayFacialEventBehaviourInstruction">
        <Delay>
          <Value>0.5</Value>
        </Delay>
        <IsBlocking>
          <Value>false</Value>
        </IsBlocking>
        <IsRequired>
          <Value>false</Value>
        </IsRequired>
        <FacialEvent>
          <Value>RELATIVE_EVENT</Value>
        </FacialEvent>
      </ABehaviourInstruction>
    </Instructions>
  </BehaviourAlgorithm>
