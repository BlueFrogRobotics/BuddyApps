<?xml version="1.0" encoding="utf-8"?>
<BehaviourAlgorithmStorage xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <Settings>
		<Category>Whistle</Category>
    <Mood>NEUTRAL</Mood>
    <Commitment>NOT_ENGAGED</Commitment>
    <MovementPattern>COMPLETE_FREEDOM</MovementPattern>
  </Settings>
  <Algorithm>
    <Instructions>
      <ABehaviourInstruction xsi:type="MoveHeadBehaviourInstruction">
        <IsBlocking>
          <Value>false</Value>
        </IsBlocking>
        <CustomMove>
          <Value>true</Value>
        </CustomMove>
        <YesAngle>
          <Value>-10</Value>
        </YesAngle>
        <YesSpeed>
          <Value>20</Value>
        </YesSpeed>
      </ABehaviourInstruction>
      <ABehaviourInstruction xsi:type="MoveHeadBehaviourInstruction">
        <IsBlocking>
          <Value>false</Value>
        </IsBlocking>
        <CustomMove>
          <Value>true</Value>
        </CustomMove>
        <NoAngle>
          <Value>-20</Value>
        </NoAngle>
        <NoSpeed>
          <Value>40</Value>
        </NoSpeed>
      </ABehaviourInstruction>
      <ABehaviourInstruction xsi:type="LookAtBehaviourInstruction">
        <IsBlocking>
          <Value>false</Value>
        </IsBlocking>
        <Position>
          <Value>TOP_LEFT</Value>
        </Position>
      </ABehaviourInstruction>
      <ABehaviourInstruction xsi:type="MoveBodyBehaviourInstruction">
        <IsBlocking>
          <Value>false</Value>
        </IsBlocking>
        <Angle>
          <Value>40</Value>
        </Angle>
        <Speed>
          <Value>80</Value>
        </Speed>
      </ABehaviourInstruction>

      <ABehaviourInstruction xsi:type="PlayFacialEventBehaviourInstruction">
        <Delay>
          <Value>1</Value>
        </Delay>
        <IsBlocking>
          <Value>false</Value>
        </IsBlocking>
        <FacialEvent>
          <Value>WHISTLE</Value>
        </FacialEvent>
      </ABehaviourInstruction>
      <ABehaviourInstruction xsi:type="PlaySoundBehaviourInstruction">
        <Delay>
          <Value>1</Value>
        </Delay>
        <IsBlocking>
          <Value>false</Value>
        </IsBlocking>
        <Sound>
          <Value>SIGH</Value>
        </Sound>
      </ABehaviourInstruction>

      <ABehaviourInstruction xsi:type="MoveBodyBehaviourInstruction">
        <Delay>
          <Value>1.2</Value>
        </Delay>
        <IsBlocking>
          <Value>false</Value>
        </IsBlocking>
        <Angle>
          <Value>-40</Value>
        </Angle>
        <Speed>
          <Value>80</Value>
        </Speed>
      </ABehaviourInstruction>
      <ABehaviourInstruction xsi:type="MoveHeadBehaviourInstruction">
        <Delay>
          <Value>1.2</Value>
        </Delay>
        <IsBlocking>
          <Value>false</Value>
        </IsBlocking>
        <CustomMove>
          <Value>true</Value>
        </CustomMove>
        <NoAngle>
          <Value>00</Value>
        </NoAngle>
        <NoSpeed>
          <Value>60</Value>
        </NoSpeed>
      </ABehaviourInstruction>
      <ABehaviourInstruction xsi:type="LookAtBehaviourInstruction">
        <Delay>
          <Value>1.2</Value>
        </Delay>
        <IsBlocking>
          <Value>false</Value>
        </IsBlocking>
        <Position>
          <Value>CENTER</Value>
        </Position>
      </ABehaviourInstruction>
      <ABehaviourInstruction xsi:type="MoveHeadBehaviourInstruction">
        <Delay>
          <Value>1.2</Value>
        </Delay>
        <IsBlocking>
          <Value>true</Value>
        </IsBlocking>
        <CustomMove>
          <Value>true</Value>
        </CustomMove>
        <YesAngle>
          <Value>10</Value>
        </YesAngle>
        <YesSpeed>
          <Value>20</Value>
        </YesSpeed>
      </ABehaviourInstruction>  
    </Instructions>
  </Algorithm>
</BehaviourAlgorithmStorage>
